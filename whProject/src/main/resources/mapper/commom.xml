<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.admin.wh.common.mapper.CommonMapper">

	<select id="commonLocal" resultType="co.admin.wh.common.vo.CommonVO">
		SELECT * FROM COMMON WHERE COM_GRO_CODE='RE'
	</select>

	<select id="commonGroup" resultType="co.admin.wh.common.vo.CommonVO">
		SELECT * FROM COMMON where com_gro_code='WG'
	</select>
	
	<select id="commonReport" resultType="co.admin.wh.common.vo.CommonVO">
		SELECT * FROM COMMON where com_gro_code='RR'
	</select>
</mapper>